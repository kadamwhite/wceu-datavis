{
  "name": "tag-adjacency-matrix",
  "description": "A visualization of tag co-occurrence",
  "author": "K Adam White",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=6.6.0"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "scripts": {
    "clean": "rimraf dist/ && mkdirp dist",
    "clear-cache": "rimraf node_modules/.cache",
    "build": "npm run clean && webpack",
    "build:prod": "npm run clean && webpack --config webpack.config.prod.js",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext js,jsx ./*.js src jest .storybook",
    "start": "mkdirp dist && webpack-dev-server",
    "storybook": "start-storybook -p 6006",
    "test": "node jest/run.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/file-stub.js",
      "^.+\\.(scss|css)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/[^\\/]*|\\.(test|spec))\\.(js|jsx)$"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "d3": "^4.9.1",
    "d3-scale-chromatic": "^1.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.3",
    "wpapi": "^1.1.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "find-cache-dir": "^1.0.0",
    "hard-source-webpack-plugin": "^0.3.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "mkdirp": "^0.5.1",
    "node-object-hash": "^1.2.0",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.5.1",
    "redux-saga-test-plan": "^3.0.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "style-loader": "^0.18.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-file-list-plugin": "0.0.6"
  }
}
