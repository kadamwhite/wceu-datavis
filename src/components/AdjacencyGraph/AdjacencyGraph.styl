.container {
  overflow: auto;
}

.background {
  fill: #eee;
}

line {
  stroke: #fff;
}

text.active {
  fill: red;
}

.row {
  stroke: 0;
}

.cell {
  stroke: lightgrey;
  stroke-width: 1px;
}
.cell-self {
  fill: white;
  stroke: black;
  stroke-width: 2px;
}

.cell,
.label,
.group {
  transition: x 1s, y 1s, fill 1s, transform 1s;
}
